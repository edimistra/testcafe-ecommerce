{
    "browsers":[
      "chrome:headless"
    ],
    "src":[
      "./tests"
    ],
    "reporter":[
      {
        "name":"spec"
      },
      {
        "name":"html",
        "output":"results/reports/report.html"
      }
    ],
    "skipJsErrors": true,
    "screenshots": {
        "path": "results/screenshots",
        "pathPattern": "${DATE}-${TIME}/${FIXTURE}/test-${TEST_INDEX}/${FILE_INDEX}.png",
        "takeOnFails":"true"
    },
    "selectorTimeout":20000,
    "assertionTimeout":20000,
    "pageLoadTimeout":10000,
    "speed":1
}
